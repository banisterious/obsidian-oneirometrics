    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "builtin-modules": "^3.2.0",
        "esbuild": "^0.20.0",
        "obsidian": "latest",
        "tslib": "2.3.1",
        "typescript": "^5.4.0"
    },
    "dependencies": {
        "date-fns": "^2.30.0",
        "lucide-static": "^0.509.0"
    }
