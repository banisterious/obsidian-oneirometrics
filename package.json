    "description": "Track and analyze your dream journal entries with metrics and visualizations.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node build-css.js",
